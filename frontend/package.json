{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sass": "sass sass/main.scss styles/globals.css -w",
    "compile:sass": "sass sass/main.scss styles/globals.comp.css",
