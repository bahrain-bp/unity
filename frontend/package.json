    "@mui/joy": "^5.0.0-beta.52",
    "@mui/material": "^7.3.5",
    "autoprefixer": "^10.4.21",
    "aws-amplify": "^6.15.8",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.562.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.0",
